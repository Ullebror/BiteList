{
    "extends": "@tsconfig/react-native/tsconfig.json",
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "esModuleInterop": true,
        "allowJs": true,
        "skipLibCheck": true,
        "resolveJsonModule": true,
        "strict": true,
        "baseUrl": "./"
      },
      "include": ["src/**/*", "App.tsx", "metro.config.*"],
      "exclude": ["node_modules", "babel.config.js"]
  }